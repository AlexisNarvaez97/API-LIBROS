{
  "name": "libroscrud",
  "version": "1.0.0",
  "description": "Crud de libros y autores",
  "main": "build/server.js",
  "scripts": {
    "start": "node build/server.js",
    "build": "tsc -p . && ncp src/schema build/schema",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon \"src/server.ts\" --exec \"ts-node\" src/server.ts -e ts,graphql,json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexisNarvez97/libroscrud.git"
  },
  "keywords": [
    "graphql",
    "graphql-curso",
    "hello-world",
    "mongodb"
  ],
  "author": "AlexisNarvaez <alexisnarvaez97@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexisNarvez97/libroscrud/issues"
  },
  "homepage": "https://github.com/AlexisNarvez97/libroscrud#readme",
  "dependencies": {
    "apollo-server-express": "^2.6.7",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "graphql": "^14.3.1",
    "graphql-import-node": "0.0.4",
    "graphql-playground-middleware-express": "^1.7.12",
    "graphql-tools": "^4.0.5",
    "http": "0.0.0",
    "mongodb": "^3.2.7",
    "ncp": "^2.0.0",
    "subscriptions-transport-ws": "^0.9.16",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/express-graphql": "^0.8.0",
    "@types/mongodb": "^3.1.28",
    "@types/node": "^12.0.10",
    "nodemon": "^2.0.1",
    "ts-node": "^8.5.4",
    "typescript-tslint-plugin": "0.5.5"
  }
}
